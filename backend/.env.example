# Google AI (Gemini) API Key - for Street View AI transformation
# Get from: https://aistudio.google.com/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Google Maps API Key - for Street View Static API
# Same key as frontend, needs "Street View Static API" enabled
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Path to the Sentinel-2 GeoTIFF heat map file (optional â€” synthetic fallback available)
HEATMAP_TIFF_PATH=data/heat_map.tif

# Default map center (Vancouver)
DEFAULT_LAT=49.2827
DEFAULT_LON=-123.1207

# Supabase (for caching AI visualizations)
# Get from: https://supabase.com/dashboard/project/_/settings/api
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_service_role_key_here
